package com.biz.var;

public class print_01 {

	public static void main(String[] args) {
		System.out.println("대한민국만세");
		System.out.println();
		System.out.println("우리나라만세");
		System.out.print(30+40);
		System.out.print(30-40);
		
		int num1 = 30;
		int num2 = 7;
		

		System.out.print(num1);
		System.out.print("/");
		System.out.print(num2);
		System.out.print("=");
		System.out.println(num1/num2);

	}

}

package com.biz.var;

public class Var_06 {

	public static void main(String[] args) {

		int danNum = 3;
		int countNum = 2;
		int tNum = 0;
		
		tNum = danNum * countNum;
		System.out.printf("%d x %d = %d\n", danNum, countNum, tNum);
		
		countNum = countNum +1;
		tNum = danNum * countNum;
		System.out.printf("%d x %d = %d\n", danNum, countNum, tNum);
	
		countNum = countNum +1;
		tNum = danNum * countNum;
		System.out.printf("%d x %d = %d\n", danNum, countNum, tNum);
		
		countNum = countNum +1;
		tNum = danNum * countNum;
		System.out.printf("%d x %d = %d\n", danNum, countNum, tNum);
	}

}

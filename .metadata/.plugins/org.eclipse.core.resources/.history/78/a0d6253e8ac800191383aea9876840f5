package com.biz.string;

public class String_05 {

	public static void main(String[] args) {

		int num = 30;
		boolean bYes = num == 30;
		
		if(bYes) 
			System.out.println("num에는 30이 담겨있다");
		else 
			System.out.println("num에는 30이 담겨있지 않다");
		
		num = 50;
		int num1 = 30;
		bYes = num == num1;
		
		
	}

}

package com.biz.classes;
/*
 * 0부터 100까지 숫자 중에서 7의 배수가 두번째로 나타나면 
 * 그 값을 표시하라
 */
public class String_10 {

	public static void main(String[] args) {

	    int nCount = 0;
		for(int i = 1; i < 100 ; i ++) {
			if(i % 7 == 0) nCount ++;
			if(nCount == 2) {
				System.out.println(i); 
		        break;
			}}
	}

}

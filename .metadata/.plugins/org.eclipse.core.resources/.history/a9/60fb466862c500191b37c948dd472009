package com.biz.var;

public class String_04 {

	public static void main(String[] args) {
		
		// 숫자를 문자열 변수에 저장하려면
		// 숫자를 문자열형으로 변환을 시켜야한다.
		// 숫자를 문자열행으로 변환시키려면
		// 숫자 + "" 형식으로 만들어주면 문자열로
		// 변환된다
		
		String num = 0 + "";
		
		int intNum1 = 30;
		num = intNum1 + ""; 
		
		num = 3 + " " + 4;
		
		System.out.println(num);

	}

}

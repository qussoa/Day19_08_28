package com.biz.var;

public class String_05 {

	public static void main(String[] args) {
		
		
		String n1 = "대";
		String n2 = "한";
		String n3 = "민";
		String n4 = "국";
		
		String nation = n1 + n2 + n3 + n4;
		System.out.println(nation);
		
		int no1 = 30;
		int no2 = 40;
		int no3 = 30+40;
		
		nation = n1+no1+no2+no3;
		System.out.println(nation);
		

	}

}

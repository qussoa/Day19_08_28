package com.biz.loop;

public class Loop_01 {

	public static void main(String[] args) {

		int mCount = 0;
		
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
		System.out.println("카운트 : " + nCount++);
	}

}

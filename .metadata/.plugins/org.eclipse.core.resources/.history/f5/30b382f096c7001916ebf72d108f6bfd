package com.biz.var;

public class Var_01 {

	public static void main(String[] args) {
		
		int num = 5;
		System.out.printf("%d x %d = %05d\n", num, 2, num*2);
		System.out.printf("%d x %d = %05d\n", num, 3, num*3);
		System.out.printf("%d x %d = %05d\n", num, 4, num*4);
		System.out.printf("%d x %d = %05d\n", num, 5, num*5);
		System.out.printf("%d x %d = %05d\n", num, 6, num*6);
		System.out.printf("%d x %d = %05d\n", num, 7, num*7);
		System.out.printf("%d x %d = %05d\n", num, 8, num*8);
		System.out.printf("%d x %d = %05d\n", num, 9, num*9);

	}

}
